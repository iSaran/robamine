<mujoco model="CLUTTER">
  <compiler
    angle="radian"
    inertiafromgeom="true" />
  <option
    iterations="10"
    timestep="0.001"
    solver="Newton"/>
  <size njmax="2000" nconmax="1000" />
  <default>
    <geom
      rgba="0.0 0.4 0.6 1" />
  </default>
  <visual>
    <map
      fogstart="1.5"
      fogend="3"
      force="0.3"/>
    <quality
      shadowsize="8192"/>
    <scale
      forcewidth="0.01"
      contactwidth="0.02"
      contactheight="0.015"
      framelength="0.3"
      framewidth="0.015"/>
  </visual>
  <worldbody>
    <camera name="xtion"
      pos="0 0 0.75"
      quat="1 0 0 0" />
    <light directional="false" cutoff="90" exponent="1" diffuse="1 1 1" specular="0 0 0" pos="3 0 3" castshadow="false"/>
    <light directional="false" cutoff="90" exponent="1" diffuse="1 1 1" specular="0 0 0" pos="0 3 3" castshadow="false"/>
    <light directional="false" cutoff="90" exponent="1" diffuse="1 1 1" specular="0 0 0" pos="-3 0 3" castshadow="false" />
    <light directional="false" cutoff="90" exponent="1" diffuse="1 1 1" specular="0 0 0" pos="0 -3 3" castshadow="false" />
    <geom
      name="floor"
      type="plane"
      pos ="0 0 -0.3"
      size="0.24 0.24 0.25"
      rgba="0.0 0.0 0.0 1"/>
  </worldbody>
  <actuator>
    <motor gear="1 0 0 0 0 0" joint="finger1" name="finger_0_wrench_actuator"/>
    <motor gear="0 1 0 0 0 0" joint="finger1" name="finger_1_wrench_actuator"/>
    <motor gear="0 0 1 0 0 0" joint="finger1" name="finger_2_wrench_actuator"/>
    <motor gear="0 0 0 1 0 0" joint="finger1" name="finger_3_wrench_actuator"/>
    <motor gear="0 0 0 0 1 0" joint="finger1" name="finger_4_wrench_actuator"/>
    <motor gear="0 0 0 0 0 1" joint="finger1" name="finger_5_wrench_actuator"/>

    <motor gear="1 0 0 0 0 0" joint="finger2" name="finger2_0_wrench_actuator"/>
    <motor gear="0 1 0 0 0 0" joint="finger2" name="finger2_1_wrench_actuator"/>
    <motor gear="0 0 1 0 0 0" joint="finger2" name="finger2_2_wrench_actuator"/>
    <motor gear="0 0 0 1 0 0" joint="finger2" name="finger2_3_wrench_actuator"/>
    <motor gear="0 0 0 0 1 0" joint="finger2" name="finger2_4_wrench_actuator"/>
    <motor gear="0 0 0 0 0 1" joint="finger2" name="finger2_5_wrench_actuator"/>
  </actuator>
</mujoco>